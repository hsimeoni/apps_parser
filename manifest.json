{
	"name" : "HSimeoni",
	"version" : "0.1",
	"manifest_version": 2,
	"description" : " Facebook Parser",
	"permissions": ["tabs", "<all_urls>"],
	"icons" : {
		"16" : "pcpro_16.png",
		"48" : "pcpro_48.jpg",
		"128" : "pcpro_128.jpg"
		},
	"browser_action" : {
		"default_icon" : "su.png",
		"default_popup" : "popup.html"
		},
	"content_scripts": [{
      "all_frames": true,
      "matches": ["<all_urls>"],
      "exclude_matches": [],
      "js": ["jquery-1.10.2.js"]
    }]
	}